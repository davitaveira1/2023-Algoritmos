algoritmo "semnome"
// Função :
// Autor :
// Data : 11/05/2023
// Seção de Declarações 
var
c : real
k : real
f : real
inicio
// Seção de Comandos 
escreva("Informe a temperatura em Celsius: ")
leia(c)

f <- c*9/5 + 32
k <- c + 273

escreval("A temperatura em f é: ",f)
escreval("A temperatura em k é: ",k)

fimalgoritmo
